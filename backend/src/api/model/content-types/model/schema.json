{
  "kind": "collectionType",
  "collectionName": "models",
  "info": {
    "singularName": "model",
    "pluralName": "models",
    "displayName": "Model"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "models"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "brochure": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "content": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    }
  }
}
